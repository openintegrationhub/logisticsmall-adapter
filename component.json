{
  "title": "logistics mall API",
  "description": "elastic.io component for connecting to logistics mall",
  "docsUrl": "https://confluence.logistics-mall.com/",
  "credentials": {
    "fields": {
     "serverURLd": {
        "label": "Managed App Server URL",
        "required": true,
        "viewClass": "TextFieldWithNoteView",
        "note": "Example URL <b>https://tenant.logistics-mall.com</b>"
      }	 
    },
    "verifier": "io.elastic.petstore.ApiKeyVerifier"
  },
  
  "triggers": {
     "getPetByStatusJaxRs": {
      "main": "io.logmall.actions.CreateShipment",
      "type": "polling",
      "title": "Get Shipment By Status (JAX-RS)",
      "description": "Retrieves shipments from the logistics mall API by given shipment status using Java API for RESTful Web Services (JAX-RS)",
      "metadata": {
        "out": "./schemas/getPetsByStatus.out.json"
      }
    }
  },
 
  "actions": {
    "createShipment": {
      "main": "io.logmall.actions.CreateShipment",
      "title": "Create a Shipment",
      "description": "Creates a new Shipment",
      "metadata": {
        "in": "./schemas/createShipment.in.json",
        "out": "./schemas/createShipment.out.json"
      }
    }
  }
}
